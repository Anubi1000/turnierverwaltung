#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/daniel/Projekt/turnierverwaltung/node_modules/.pnpm/git-format-staged@3.1.1/node_modules/git-format-staged/node_modules:/home/daniel/Projekt/turnierverwaltung/node_modules/.pnpm/git-format-staged@3.1.1/node_modules:/home/daniel/Projekt/turnierverwaltung/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/daniel/Projekt/turnierverwaltung/node_modules/.pnpm/git-format-staged@3.1.1/node_modules/git-format-staged/node_modules:/home/daniel/Projekt/turnierverwaltung/node_modules/.pnpm/git-format-staged@3.1.1/node_modules:/home/daniel/Projekt/turnierverwaltung/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/python3" ]; then
  exec "$basedir/python3"  "$basedir/../git-format-staged/git-format-staged" "$@"
else
  exec python3  "$basedir/../git-format-staged/git-format-staged" "$@"
fi

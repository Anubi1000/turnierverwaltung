    "@vue/test-utils": "2.4.6",
    "happy-dom": "17.4.4",
    "openapi-typescript": "7.6.1",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "typescript": "5.8.2",
    "vite-plugin-vue-devtools": "7.7.2",
    "vitest": "3.0.9"
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": "22.x"

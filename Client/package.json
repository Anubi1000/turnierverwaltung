    "genApiTypes": "openapi-typescript http://localhost:5097/openapi/v1.json -o ./app/utils/api-types.ts -t --root-types --root-types-no-schema-prefix"
  },
  "dependencies": {
    "@nuxt/eslint": "1.2.0",
    "@nuxt/fonts": "0.11.0",
    "@nuxt/icon": "1.11.0",
    "@pinia/nuxt": "0.10.1",
    "@primevue/themes": "4.2.5",
    "@tailwindcss/vite": "4.0.17",
    "@tanstack/vue-query": "5.69.0",
    "@vee-validate/nuxt": "4.15.0",
    "@vee-validate/zod": "4.15.0",
    "eslint": "9.23.0",
    "lodash-es": "4.17.21",
    "nuxt": "3.16.1",
    "pinia": "3.0.1",
    "primevue": "4.2.5",
    "tailwindcss": "4.0.17",
    "tailwindcss-primeui": "0.5.1",
    "vee-validate": "4.15.0",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "zod": "3.24.2"
